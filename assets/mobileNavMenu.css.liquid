.levelOneMenu{
	border-bottom: 3px solid #dfdfdf;;
    list-style: none;
    padding-top: 10px;
    padding-bottom: 10px;
}

.openMenu{
	display: block!important;
}

.menuMobileWrap{
	margin:0px;
}

.mobmenuLink{
	  color: #1d1d1d;
}

.has-dropdown-levelone{
 	height: 20px;
    overflow: hidden;
    position: relative;
	-webkit-transition: height 0.1s;
	-moz-transition: height 0.1s;
	transition: height 0.1s;
}

.has-dropdown-leveltwo{
    height: 20px;
    overflow: hidden;
    position: relative;
}

