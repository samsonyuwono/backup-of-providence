}

.levelthreelistItem{
	list-style: none;
}

.spanlevelOne{
	padding:10px;
}

.spanlevelTwo{
	margin-left: 20px;
}

.spanlevelThree{
	margin-left: 30px;
}

.has-dropdown-levelone:before{
    background-image:url({{'arrow-down-icon.png' | asset_url}});
    content: '';
    display: inline-block;
    width: 25px;
    height: 25px;
    background-color: transparent;
    background-size: 18px;
    background-repeat: no-repeat;
    right: 0;
    margin-top: 5px;
    position: absolute;
}

.has-dropdown-leveltwo:before{
	background-image:url({{'mobileDropPlus.png' | asset_url}});
    content: '';
    display: inline-block;
    width: 25px;
    height: 25px;
    background-color: transparent;
    background-size: 18px;
    background-repeat: no-repeat;
    right: -1px;
    margin-top: 0px;
    position: absolute;

}

